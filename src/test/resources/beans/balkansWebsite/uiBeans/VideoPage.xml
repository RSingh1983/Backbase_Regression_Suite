<?xml version="1.0"?>

<UIElements>

    <Element type="input">

        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

    </Element>

    <Element type="combo">

        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

    </Element>

    <Element type="click">

        <selector>
            <keyword>Header_Menu_Video</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//div[@class='desktop-header visible-over-tablet']//section[@id='block-menu-block-aj-balkans-main-menu']//a[text()='Video']</element_locator_value>
        </selector>

        <selector>
            <keyword>MainSection_Video</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h1[@class='page-header' and text()='Video']</element_locator_value>
        </selector>

        <!-- Selectors for various Sections-->
        <selector>
            <keyword>SubSection1_BALKAN</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h4[text()='BALKAN']</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection2_SVIJET</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h4[text()='SVIJET']</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection3_EKONOMIJA</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h4[text()='EKONOMIJA']</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection4_SPORT</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h4[text()='SPORT']</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection5_GOSTI</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h4[text()='GOSTI']</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection6_REPORTERI</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h4[text()='REPORTERI']</element_locator_value>
        </selector>

    </Element>

    <Element type="text">

        <selector>
            <element_locator_type>css</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

    </Element>

</UIElements>