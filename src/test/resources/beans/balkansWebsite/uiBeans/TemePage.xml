<?xml version="1.0"?>

<UIElements>

    <Element type="input">

        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

    </Element>

    <Element type="combo">

        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

    </Element>

    <Element type="click">

        <selector>
            <keyword>Header_Menu_Teme</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//div[@class='desktop-header visible-over-tablet']//section[@id='block-menu-block-aj-balkans-main-menu']//a[text()='Teme' and @href='/kategorije/teme']</element_locator_value>
        </selector>

        <selector>
            <keyword>MainSection_TEME</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h1[@class='page-header' and text()='TEME']</element_locator_value>
        </selector>

        <!-- Selectors for various Sections-->
        <selector>
            <keyword>SubSection1_SVE</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//span[text()='SVE']</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection2_ÄŒlanci</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//span[text()='SVE']</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection3_Video</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//span[text()='SVE']</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection4_Blog</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//span[text()='SVE']</element_locator_value>
        </selector>

        <selector>
            <keyword>Page_Headers</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//header[@id='navbar']</element_locator_value>
        </selector>

        <selector>
            <keyword>Header_Anchors</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//header[@id='navbar']//a</element_locator_value>
        </selector>

        <selector>
            <keyword>Main_Page_Anchors</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//section[@id='block-system-main']//a</element_locator_value>
        </selector>

        <selector>
            <keyword>Footer_Anchors</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//footer[@class='content-bottom']//a</element_locator_value>
        </selector>

        <selector>
            <keyword>Header_Images</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//header[@id='navbar']//a/img</element_locator_value>
        </selector>

        <selector>
            <keyword>Main_Page_Images</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//section[@id='block-system-main']//img[not(ancestor-or-self::*[(contains(@class, 'hide'))])]</element_locator_value>
        </selector>

        <selector>
            <keyword>Main_Page_Image_PlaceHolder</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//section[@id='block-system-main']//img[@src='_##_' and not(ancestor-or-self::*[(contains(@class, 'hide'))])]</element_locator_value>
        </selector>

        <selector>
            <keyword>Footer_Images</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//footer[@class='content-bottom']//img[not(ancestor-or-self::*[(contains(@class, 'hide'))])]</element_locator_value>
        </selector>

        <selector>
            <keyword>Footer_Image_PlaceHolder</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//footer[@class='content-bottom']//img[@src='_##_' and not(ancestor-or-self::*[(contains(@class, 'hide'))])]</element_locator_value>
        </selector>

        <selector>
            <keyword>ImagePlaceholderForVisibility</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//img[@src='_##_']</element_locator_value>
        </selector>

    </Element>

    <Element type="text">

        <!-- Selectors for getting all the visible hyperlinks on the page-->
        <selector>
            <keyword>All_Hyperlinks</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//section[@id='block-system-main']//a[not(ancestor-or-self::*[(contains(@class, 'hide'))]) and not(ancestor-or-self::*[(contains(@class, 'field-label-hidden'))])]</element_locator_value>
        </selector>

    </Element>

</UIElements>