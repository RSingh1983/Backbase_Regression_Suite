<?xml version="1.0"?>

<UIElements>

    <Element type="input">

        <!-- Search Text Box Selectors-->
        <selector>
            <keyword>Search_Computer_Textbox</keyword>
            <element_locator_type>id</element_locator_type>
            <element_locator_value>searchbox</element_locator_value>
        </selector>

    </Element>

    <Element type="combo">

    </Element>

    <Element type="click">

        <selector>
            <keyword>Add_New_Computer_Button</keyword>
            <element_locator_type>id</element_locator_type>
            <element_locator_value>add</element_locator_value>
        </selector>

        <selector>
            <keyword>Filter_Computer_Button</keyword>
            <element_locator_type>id</element_locator_type>
            <element_locator_value>searchsubmit</element_locator_value>
        </selector>

        <selector>
            <keyword>ComputerName</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//tr/td/a[text()='_##_']</element_locator_value>
        </selector>

    </Element>

    <Element type="text">
        <selector>
            <keyword>Add_Computer_Success_Message</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//div[@class='alert-message warning']</element_locator_value>
        </selector>

        <selector>
            <keyword>NumberOfSearchedResults</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//table/tbody/tr</element_locator_value>
        </selector>

        <selector>
            <keyword>NoSearchedResults</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//em[text()='Nothing to display']</element_locator_value>
        </selector>

        <selector>
            <keyword>NoSearchedResultsLabel</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//section/h1</element_locator_value>
        </selector>

        <selector>
            <keyword>ComputerDetails</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//tr[1]/td[_##_]</element_locator_value>
        </selector>

    </Element>

</UIElements>