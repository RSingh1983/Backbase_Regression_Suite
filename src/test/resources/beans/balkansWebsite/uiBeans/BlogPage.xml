<?xml version="1.0"?>

<UIElements>

    <Element type="input">

        <selector>
            <element_locator_type>id</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

    </Element>

    <Element type="combo">

        <selector>
            <element_locator_type>xpath</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

    </Element>

    <Element type="click">

        <selector>
            <keyword>Header_Menu_Blog</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//div[@class='desktop-header visible-over-tablet']//section[@id='block-menu-block-aj-balkans-main-menu']//a[text()='Blog']</element_locator_value>
        </selector>

        <selector>
            <keyword>MainSection_Blog</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h1[@class='page-header' and text()='Blog']</element_locator_value>
        </selector>

        <!-- Selectors for various Sections-->
        <selector>
            <keyword>SubSection1_Popular</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h2[@class='pane-title' and contains(text(),'Popular')]</element_locator_value>
        </selector>

        <selector>
            <keyword>SubSection2_Top_blogeri</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//h2[@class='pane-title' and (contains(text(),'najpopularniji blogeri') or contains(text(),'Top blogeri'))]</element_locator_value>
        </selector>

        <selector>
            <keyword>Page_Headers</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//header[@id='navbar']</element_locator_value>
        </selector>

        <selector>
            <keyword>Header_Anchors</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//header[@id='navbar']//a</element_locator_value>
        </selector>

        <selector>
            <keyword>Main_Page_Anchors</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//section[@id='block-system-main']//a</element_locator_value>
        </selector>

        <selector>
            <keyword>Footer_Anchors</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//footer[@class='content-bottom']//a</element_locator_value>
        </selector>

        <selector>
            <keyword>Header_Images</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//header[@id='navbar']//a/img</element_locator_value>
        </selector>

        <selector>
            <keyword>Main_Page_Images</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//section[@id='block-system-main']//a/img</element_locator_value>
            <!--<selector_debug>All_Images</selector_debug>-->
        </selector>

        <selector>
            <keyword>Footer_Images</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//footer[@class='content-bottom']//a/img</element_locator_value>
        </selector>

        <selector>
            <keyword>ImagePlaceholderForVisibility</keyword>
            <element_locator_type>xpath</element_locator_type>
            <element_locator_value>//img[@src='_##_']</element_locator_value>
        </selector>

    </Element>

    <Element type="text">

        <selector>
            <element_locator_type>css</element_locator_type>
            <keyword></keyword>
            <element_locator_value></element_locator_value>
        </selector>

    </Element>

</UIElements>